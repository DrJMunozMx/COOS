#################################################################
# Ontology
#################################################################

<http://id.unece.org/def/coos> a owl:Ontology, voaf:Vocabulary ;
    dc:title "COOS: a core ontology for Official Statistics"@en , "COOS : une ontologie centrale pour la Statistique Officielle"@fr ;
    dc:contributor "Guillaume Duffes" , "Dan Gillman" , "Flavio Rizzolo" , "Francine Kalonji" ;
    dc:creator "Franck Cotton" ;
    vann:preferredNamespacePrefix "coos" ;
    vann:preferredNamespaceUri <http://id.unece.org/def/coos#> ;
    dc:rights "Copyright © 2019 Unece" ;
    cc:license <http://creativecommons.org/licenses/by/4.0/> ;
    voaf:classNumber "9"^^xsd:integer ;
    voaf:propertyNumber "0"^^xsd:integer ;
    owl:versionInfo "Version 0.1 - 2019-04-04"  .


#################################################################
# Classes
#################################################################

# Classes related to the 'activity' domain

coos:Activity a rdfs:Class , owl:Class ;
    rdfs:label "Activity"@en , "Activité"@fr ;
    rdfs:comment "An activity as defined by the GAMSO."@en , "Une activité telle que définie par le GAMSO."@fr ;
    rdfs:subClassOf skos:Concept , prov:Activity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:ActivityArea a rdfs:Class , owl:Class ;
    rdfs:label "Activity area"@en , "Secteur d'activité"@fr ;
    rdfs:comment "An activity area as defined by the GAMSO."@en , "Un secteur d'activité telle que définie par le GAMSO."@fr ;
    rdfs:subClassOf skos:Concept ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:StatisticalActivity a rdfs:Class , owl:Class ;
    rdfs:label "Statistical activity"@en , "Activité statistique"@fr ;
    rdfs:comment "Generic statistical activity."@en , "Activité statistique générique."@en ;
    rdfs:subClassOf coos:Activity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:StatisticalProductionActivity a rdfs:Class , owl:Class ;
    rdfs:label "Statistical production activity"@en , "Activité de production statistique"@fr ;
    rdfs:comment "Statistical production activity covered by the GSBPM."@en , "Activité de production statistique couverte par le GSBPM."@fr ;
    rdfs:subClassOf coos:StatisticalActivity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:OverarchingActivity a rdfs:Class , owl:Class ;
    rdfs:label "Overarching statistical production activity"@en , "Activité de production statistique transverse"@fr ;
    rdfs:comment "Activity defined as overarching process by the GSBPM."@en , "Activité définie comme processus transverse par le GSBPM."@fr ;
    rdfs:subClassOf coos:StatisticalProductionActivity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:Phase a rdfs:Class , owl:Class ;
    rdfs:label "Statistical production process phase"@en , "Phase du processus de production statistique"@fr ;
    rdfs:comment "Process phase as defined by the GSBPM."@en , "Phase du processus telle que définie par le GSBPM."@fr ;
    rdfs:subClassOf coos:StatisticalProductionActivity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:SubProcess a rdfs:Class , owl:Class ;
    rdfs:label "Statistical production sub-process"@en , "Sous-processus de production statistique"@fr ;
    rdfs:comment "Sub-process as defined by the GSBPM."@en , "Sous-processus tel que défini par le GSBPM."@fr ;
    rdfs:subClassOf coos:StatisticalProductionActivity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:StatisticalProgram a rdfs:Class , owl:Class ;
    rdfs:label "Statistical program"@en , "Programme statistique"@fr ;
    rdfs:comment "As defined by the GSIM."@en , "Tel que défini par le GSIM."@fr ;
    rdfs:subClassOf coos:Activity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .

coos:StatisticalProgramCycle a rdfs:Class , owl:Class ;
    rdfs:label "Statistical program cycle"@en , "Cycle de programme statistique"@fr ;
    rdfs:comment "As defined by the GSIM."@en , "Tel que défini par le GSIM."@fr ;
    rdfs:subClassOf coos:Activity ;
    rdfs:isDefinedBy <http://id.unece.org/def/coos> .


